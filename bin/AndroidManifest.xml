<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cmu.maccha"
    android:versionCode="1"
    android:versionName="1.0" >
 <uses-permission android:name="android.permission.INTERNET" />
    
    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="15" />
    
    
   
   
    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        <activity android:name=".MyOrdersActivity" />
        <activity android:name=".WatchlistActivity" />
     	<activity android:name=".SettingsActivity" />
      	<activity android:name=".ActiveBidsActivity"  />
        
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MyCha"
            android:label="@string/title_activity_my_cha" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.cmu.chimps.casa.MainActivity" />
        </activity>
       
      
    </application>

</manifest>